<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingHorizontal="20dp"
    android:paddingTop="20dp"
    android:paddingBottom="10dp"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/layout_title_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_result_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Run # - "
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tv_result_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Data yang digunakan: "
            android:fontFamily="@font/inter"
            android:textColor="@color/neutral_black"
            android:textSize="14sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_first_descriptive_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_descriptive_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Deskriptif"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_descriptive_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="values"
            android:fontFamily="@font/inter"
            android:textColor="@color/neutral_black"
            android:textSize="14sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_second_descriptive_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_second_descriptive_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Deskriptif"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_second_descriptive_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="values"
            android:fontFamily="@font/inter"
            android:textColor="@color/neutral_black"
            android:textSize="14sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_third_descriptive_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_third_descriptive_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Deskriptif"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_third_descriptive_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="values"
            android:fontFamily="@font/inter"
            android:textColor="@color/neutral_black"
            android:textSize="14sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_test_values_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_test_values_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Test Values"
            android:fontFamily="@font/inter_bold"
            android:textColor="@color/blue"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_test_values_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:text="values"
            android:fontFamily="@font/inter"
            android:textColor="@color/neutral_black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_result_conclusion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Conclusion"
            android:fontFamily="@font/inter_semibold"
            android:textColor="@color/neutral_black"
            android:textSize="14sp" />
    </LinearLayout>

    <View
        android:id="@+id/view_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="@color/gray_50" />
</LinearLayout>